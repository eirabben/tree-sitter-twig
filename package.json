{
  "name": "tree-sitter-test",
  "version": "1.0.0",
  "main": "bindings/node",
  "license": "MIT",
  "dependencies": {
    "nan": "^2.15.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.20.4",
    "tree-sitter-html": "github:tree-sitter/tree-sitter-html#d93af48"
  },
  "tree-sitter": [
    {
      "scope": "text.html.twig",
      "file-types": [
        "twig"
      ],
      "highlights": [
        "node_modules/tree-sitter-html/queries/highlights.scm"
      ],
      "injections": "node_modules/tree-sitter-html/queries/injections.scm",
      "injection-regex": "twig"
    }
  ]
}
